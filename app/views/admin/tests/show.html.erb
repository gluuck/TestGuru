<h1><%= @test.title%></h1>
<tr>
<% @questions.each do |question|%><br>
  <td>Question: <%= link_to "#{question.body}", admin_question_path(question)%></td>
  <td><%= link_to 'Edit', edit_admin_question_path(question)%>
      <%= link_to 'Delete',
          admin_question_path(question),
          method: :delete,
          data: {confirm: 'Are yuu sure?'}%>
  </td>
</tr>
<%end%> <br><br>
<%= link_to 'Back', admin_tests_path%><br>

<%= link_to 'New question', new_admin_test_question_path(@test)%><br>
