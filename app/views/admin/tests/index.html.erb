<div class="row mt-100%">
  <div class="col col-md-100%">
    <h1><%= t('.header') %></h1>
    <%= render 'shared/errors', resource: @test%>
    <table class="table table-primary">
      <tr>
        <th class="sort-by-title pointer"><%= t('.title')%>
          <%= octicon 'arrow-up', class: 'text-success hide' %>
          <%= octicon 'arrow-down', class: 'text-success hide' %>
        </th>
        <th><%= t('.level')%></th>
        <th colspan='4'><%= t('.action')%></th>
      </tr>
      <%= render @tests%>
    </table>
    <%= link_to t('.new_test'), new_admin_test_path%>
  </div>
  <%= link_to t('.gists'), admin_gists_path %>
</div>
